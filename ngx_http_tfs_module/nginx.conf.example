#user root;
worker_processes 8;

events {
    worker_connections  1024;
}

http { 
    include       mime.types;
    default_type  application/octet-stream;
    
    sendfile        on;
    keepalive_timeout  15;

    server {
        listen       80;
        server_name  localhost;

        tfs_nsip '10.20.134.195:10000';        
    
        location = /get {   
            tfs_get;
            tfs_nsip '10.20.134.195:10000';        
        }   
    }
}
